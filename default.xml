<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <!-- You can have as many remote repositories as you like -->
  <remote fetch="git://github.com/bmwcarit" name="bmwcarit"/>
  <remote fetch="git://github.com/gumstix" name="gumstix"/>
  <remote fetch="git://git.openembedded.org" name="oe"/>
  <remote fetch="git://git.yoctoproject.org" name="yocto"/>
  <remote fetch="ssh://git@github.com/alex-wells" name="minialert" />

  <default remote="gumstix" revision="default"/>
  <!-- List all the repositories you want to fetch -->
  <!-- "name": git repository's name on hosting server
       "remote": the remote host from above (if not the default)
       "revision": commitish to checkout (if not the default)
       "path": directory where git repository gets cloned/synced -->
  <project name="meta-gumstix" path="poky/meta-gumstix" upstream="refs/heads/fido"/>
  <project name="meta-gumstix-extras" path="poky/meta-gumstix-extras" upstream="refs/heads/fido"/>
  <project name="meta-java" path="poky/meta-java" remote="yocto" upstream="refs/heads/master"/>
  <project name="meta-openembedded" path="poky/meta-openembedded" remote="oe" upstream="refs/heads/fido"/>
  <project name="meta-ros" path="poky/meta-ros" remote="bmwcarit" upstream="refs/heads/master"/>
  <project name="meta-ti" path="poky/meta-ti" remote="yocto" upstream="refs/heads/fido"/>
  <project name="meta-minialert" path="poky/meta-minialert" remote="minialert" upstream="refs/heads/master"/>
  <project name="poky" remote="yocto" upstream="refs/heads/fido"/>
</manifest>

